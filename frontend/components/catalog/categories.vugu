<div>
    <button
        vg-if="c.OnClickNew != nil"
        class="responsive extra small-margin"
        @click="c.OnClickNew()"
        >+</button>
    <button
        vg-for="_, category := range c.Categories"
        :class="c.classes(category)"
        @click="c.OnClick(category)"
        vg-content="category.Name"
    ></button>
</div>
